{
  "name": "maze-game-3d",
  "version": "2.0.0",
  "description": "A modern 3D maze game built with Three.js featuring rewards, traps, minimap, and multiple levels",
  "keywords": [
    "maze",
    "game",
    "3d",
    "three.js",
    "webgl",
    "puzzle",
    "adventure",
    "javascript",
    "vite"
  ],
  "author": "Maze Game Developer",
  "license": "MIT",
  "homepage": "https://github.com/yourusername/maze-game-3d",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/maze-game-3d.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/maze-game-3d/issues"
  },
  "scripts": {
    "dev": "vite --host --port 3000",
    "build": "vite build",
    "preview": "vite preview --host --port 4173",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier --write src/**/*.{js,css}",
    "analyze": "vite-bundle-analyzer",
    "deploy": "npm run build && gh-pages -d dist",
    "clean": "rimraf dist node_modules/.vite",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@types/three": "^0.160.0",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitest/ui": "^1.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vite": "^5.4.19",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-gh-pages": "^1.0.1",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
